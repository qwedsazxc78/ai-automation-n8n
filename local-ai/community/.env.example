# Database Configuration
POSTGRES_USER=n8n
POSTGRES_PASSWORD=n8npass
POSTGRES_DB=n8n
POSTGRES_PORT=5432

# n8n Configuration
N8N_PORT=5678
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=adminpass
N8N_HOST=localhost
WEBHOOK_URL=http://localhost:5678

# Docker Hub Configuration
# your-dockerhub-username
DOCKER_USERNAME=alexhsieh8888
IMAGE_NAME=n8n-community
IMAGE_TAG=latest

# n8n Base Image Version
N8N_VERSION=1.117.3

# Community Nodes Configuration
# Space-separated list of npm packages to install
# Find more nodes at: https://github.com/restyler/awesome-n8n
# Default: Core integration nodes
COMMUNITY_NODES="n8n-nodes-mcp n8n-nodes-linewebhook n8n-nodes-ragic"

# Additional recommended nodes (uncomment to add):
# COMMUNITY_NODES="n8n-nodes-mcp n8n-nodes-linewebhook n8n-nodes-ragic n8n-nodes-discord-trigger n8n-nodes-qrcode n8n-nodes-webpage-content-extractor n8n-nodes-document-generator n8n-nodes-pdfco @tavily/core n8n-nodes-edit-image-plus n8n-nodes-qdrant"

# Build Platforms
# Comma-separated list of platforms for multi-arch builds
# Common options: linux/amd64, linux/arm64, linux/arm/v7
PLATFORMS=linux/amd64,linux/arm64
